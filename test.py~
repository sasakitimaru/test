def sign(x):
    if x>0:
        return 1
    elif x<0:
        return -1
    else:
        return 0
def f(x):
    return x**2-3*x+2
def nibunn(a,b):
    float c
    for i in range(5):
        c = f((a+b)/2)
        if abs(f(c))<0.0001:
            return c:
        if sign(f(a)) == sign(f(c)):
            a = c
        elif sign(f(b)) == sign(f(c)):
            b = c
    return c
print(nibunn(-1,3/2))
